<%- include("partials/header") %> 

    <div class="container">
        <h1>Contact Us</h1>
        <div class="contact">
            <div id="contact-box">
                <form method="POST" action="https://formsubmit.co/88a95d4a1fbdfbd3663ce943a5568e8a ">
                    <div class="form-group concss">
                        <label for="name" >Name</label>
                        <input type="text" name="name" id="name"  required>
                    </div>
                    <div class="form-group concss">
                        <label for="email" >Email</label>
                        <input type="text" name="email" id="email"  required>
                    </div> 
                    <div class="form-group concss">
                        <label for="name" >Phone number </label>
                        <input type="integer" name="phone" id="phone"  required>
                    </div>
                    <div class="form-group concss">
                        <label for="name" >Message</label>
                        <textarea name="message" id="message" cols="10" rows="1"></textarea>
                    </div>
                </div>
                <div class="form-group concss" id="submit">
                    <button  class="btn">Submit</button>
                </div>
            </form>
        </div>
    </div>


<%- include("partials/footer") %> 